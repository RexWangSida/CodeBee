program guess_game() do

  num := input()
  guess := 50

  if (num >= 3 and num <= 97)
  do
    output (guess)

    while (guess != num)
    do
      if (guess > actual)
        guess := int:(guess - guess/2)
      else
        guess := int:(guess + guess/2)

      output (guess)
    end

    output (guess)
    return (guess)
  end
  else
  do
    output ('Number must be between 3 and 97')
    return
  end
end
